{
  "name": "yarle-evernote-to-md",
  "version": "2.9.2",
  "description": "Yet Another Rope Ladder from Evernote",
  "keywords": [
    "evernote",
    "enex",
    "converter",
    "md",
    "markdown",
    "enex to md"
  ],
  "main": "./dropTheRope.js",
  "scripts": {
    "build": "tsc --project ./src/tsconfig.json",
    "coverage": "nyc mocha",
    "lint": "tslint --project ./src/tsconfig.json",
    "start": "node --max-old-space-size=1024 ./dist/dropTheRope.js",
    "test": "nyc --reporter=html --reporter=text mocha",
    "test-watch": "mocha --watch",
    "postinstall": "node build.js"
  },
  "author": "Akos Balasko",
  "dependencies": {
    "@types/fs-extra": "8.1.0",
    "chai": "4.2.0",
    "execSync": "1.0.2",
    "fast-xml-parser": "3.12.5",
    "fs-extra": "4.0.2",
    "he": "1.2.0",
    "joplin-turndown-plugin-gfm": "1.0.12",
    "jsdom": "^16.4.0",
    "lodash": "4.17.19",
    "minimist": "1.2.5",
    "moment": "2.22.2",
    "proxyquire": "2.1.3",
    "showdown": "1.9.1",
    "ts-node": "8.5.4",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.13.0",
    "tslint-microsoft-contrib": "6.2.0",
    "turndown": "https://github.com/akosbalasko/turndown/tarball/master",
    "typescript": "3.8.3",
    "xml-flow": "^1.0.4",
    "xml-stream": "^0.4.5"
  },
  "engines": {
    "node": ">=10.18.1",
    "npm": ">=5.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akosbalasko/yarle"
  },
  "homepage": "https://github.com/akosbalasko/yarle",
