  "devDependencies": {
    "@types/he": "1.1.1",
    "@types/jsdom": "^16.2.4",
    "@types/lodash": "^4.14.151",
    "@types/minimist": "1.2.0",
    "@types/mocha": "7.0.2",
    "@types/node": "14.0.1",
    "@types/proxyquire": "1.3.28",
    "@types/showdown": "1.9.3",
    "@types/sinon": "9.0.1",
    "mocha": "5.2.0",
    "nyc": "13.0.1",
    "sinon": "9.0.2",
    "timezone-mock": "1.1.1"
  }
